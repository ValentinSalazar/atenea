<svg id="menuBtn" width="24" height="14" viewBox="0 0 34 24" fill="none" xmlns="http://www.w3.org/2000/svg">
    <rect x="34" width="4" height="34" rx="2" transform="rotate(90 34 0)" fill="white"/>
    <rect x="34" y="9.99997" width="4" height="26" rx="2" transform="rotate(90 34 9.99997)" fill="white"/>
    <rect x="34" y="20" width="4" height="18" rx="2" transform="rotate(90 34 20)" fill="white"/>
</svg>

<script>
    const menuBtn = document.querySelector('#menuBtn');
    const menuMobile = document.querySelector('#menuMobile');
    const closeMenuBoton = document.querySelector('#closeMenuBtn');

    menuBtn.addEventListener('click', () => {
        menuMobile.classList.toggle('hidden');
    });

    closeMenuBoton.addEventListener('click', () => {
        menuMobile.classList.toggle('hidden')
    });

    

</script>